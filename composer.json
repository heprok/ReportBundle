{
    "name": "tlc/report-bundle",
    "version": "1.0.0",
    "type": "symfony-bundle",
    "description": "TechnoLesCom report bundle",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "qipsius/tcpdf-bundle": "^2.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Tlc\\ReportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tlc\\ReportBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test" : "./vendor/bin/simple-phpunit"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
